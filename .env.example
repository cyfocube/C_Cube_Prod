# Environment Variables for C-Cube Tutorial Wallet

# Build Environment
NODE_ENV=development
REACT_APP_NODE_ENV=development

# Application Configuration
REACT_APP_APP_NAME=C-Cube Tutorial Wallet
REACT_APP_VERSION=1.0.0

# Build Configuration
GENERATE_SOURCEMAP=true
REACT_APP_BUILD_DATE=

# Web3 Configuration (Educational)
REACT_APP_DEFAULT_NETWORK=ethereum
REACT_APP_ENABLE_TESTNETS=true

# Security Configuration
REACT_APP_ENABLE_DEV_TOOLS=true

# Analytics (Optional)
# REACT_APP_GA_TRACKING_ID=
# REACT_APP_ANALYTICS_ENABLED=false

# Error Monitoring (Optional)
# REACT_APP_SENTRY_DSN=

# Feature Flags
REACT_APP_ENABLE_WEB_WARNING=true
REACT_APP_ENABLE_EDUCATIONAL_DISCLAIMERS=true

# Development Configuration
BROWSER=none
REACT_APP_DEV_MODE=true

# Build Optimization
INLINE_RUNTIME_CHUNK=false